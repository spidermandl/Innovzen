<?xml version="1.0" encoding="utf-8"?>
<!-- 时间选择界面 -->

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:layout="http://schemas.android.com/apk/res/com.innovzen.o2chair"
    android:layout_width="wrap_content"
    android:layout_height="match_parent"
    android:background="@drawable/background" >

    <com.innovzen.ui.MeasureView
        android:id="@+id/left_mea_vertical_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        layout:orientation="true"
        layout:rate_down="45"
        layout:rate_up="5" />

    <com.innovzen.ui.MeasureView
        android:id="@+id/left_mea_vertical_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@id/left_mea_vertical_1"
        layout:orientation="true"
        layout:rate_down="45"
        layout:rate_up="15" />

    <com.innovzen.ui.MeasureView
        android:id="@+id/left_mea_vertical_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@id/left_mea_vertical_2"
        layout:orientation="true"
        layout:rate_down="45"
        layout:rate_up="5" />

    <com.innovzen.ui.MeasureView
        android:id="@+id/left_mea_vertical_4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@id/left_mea_vertical_3"
        layout:orientation="true"
        layout:rate_down="45"
        layout:rate_up="15" />

    <com.innovzen.ui.MeasureView
        android:id="@+id/left_mea_vertical_5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@id/left_mea_vertical_4"
        layout:orientation="true"
        layout:rate_down="45"
        layout:rate_up="5" />

    <com.innovzen.ui.MeasureView
        android:id="@+id/up_mea_Horizontal_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@id/left_mea_vertical_1"
        layout:orientation="false"
        layout:rate_down="68"
        layout:rate_up="12" />

    <com.innovzen.ui.MeasureView
        android:id="@+id/up_mea_Horizontal_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@id/up_mea_Horizontal_1"
        layout:orientation="false"
        layout:rate_down="68"
        layout:rate_up="3" />

    <com.innovzen.ui.MeasureView
        android:id="@+id/up_mea_Horizontal_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@id/up_mea_Horizontal_2"
        layout:orientation="false"
        layout:rate_down="68"
        layout:rate_up="14" />

    <com.innovzen.ui.MeasureView
        android:id="@+id/up_mea_Horizontal_4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@id/up_mea_Horizontal_3"
        layout:orientation="false"
        layout:rate_down="68"
        layout:rate_up="4" />

    <com.innovzen.ui.MeasureView
        android:id="@+id/up_mea_Horizontal_5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@id/up_mea_Horizontal_4"
        layout:orientation="false"
        layout:rate_down="68"
        layout:rate_up="14" />

    <com.innovzen.ui.MeasureView
        android:id="@+id/up_mea_Horizontal_6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@id/up_mea_Horizontal_5"
        layout:orientation="false"
        layout:rate_down="68"
        layout:rate_up="4" />

    <com.innovzen.ui.MeasureView
        android:id="@+id/up_mea_Horizontal_7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@id/up_mea_Horizontal_6"
        layout:orientation="false"
        layout:rate_down="68"
        layout:rate_up="14" />

    <com.innovzen.ui.MeasureView
        android:id="@+id/up_mea_Horizontal_8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@id/up_mea_Horizontal_7"
        layout:orientation="false"
        layout:rate_down="68"
        layout:rate_up="3" />

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@id/left_mea_vertical_5"
        android:layout_alignLeft="@id/up_mea_Horizontal_1"
        android:layout_alignRight="@id/up_mea_Horizontal_1"
        android:layout_alignTop="@id/left_mea_vertical_1"
        android:background="#000000" >

        <include layout="@layout/reusable_lefter" />
    </LinearLayout>

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@id/left_mea_vertical_2"
        android:layout_alignLeft="@id/up_mea_Horizontal_3"
        android:layout_alignRight="@id/up_mea_Horizontal_3"
        android:layout_alignTop="@id/left_mea_vertical_2"
        android:background="@drawable/selector_time_5min" />
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@id/left_mea_vertical_2"
        android:layout_alignLeft="@id/up_mea_Horizontal_5"
        android:layout_alignRight="@id/up_mea_Horizontal_5"
        android:layout_alignTop="@id/left_mea_vertical_2"
        android:background="@drawable/selector_time_10min" />
<ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@id/left_mea_vertical_2"
        android:layout_alignLeft="@id/up_mea_Horizontal_7"
        android:layout_alignRight="@id/up_mea_Horizontal_7"
        android:layout_alignTop="@id/left_mea_vertical_2"
        android:background="@drawable/selector_time_15min" />
<ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@id/left_mea_vertical_4"
        android:layout_alignLeft="@id/up_mea_Horizontal_3"
        android:layout_alignRight="@id/up_mea_Horizontal_3"
        android:layout_alignTop="@id/left_mea_vertical_4"
        android:background="@drawable/selector_time_20min" />
<ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@id/left_mea_vertical_4"
        android:layout_alignLeft="@id/up_mea_Horizontal_5"
        android:layout_alignRight="@id/up_mea_Horizontal_5"
        android:layout_alignTop="@id/left_mea_vertical_4"
        android:background="@drawable/selector_time_25min" />
<ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@id/left_mea_vertical_4"
        android:layout_alignLeft="@id/up_mea_Horizontal_7"
        android:layout_alignRight="@id/up_mea_Horizontal_7"
        android:layout_alignTop="@id/left_mea_vertical_4"
        android:background="@drawable/selector_time_30min" />
</RelativeLayout>