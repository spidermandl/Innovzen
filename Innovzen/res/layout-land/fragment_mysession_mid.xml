<?xml version="1.0" encoding="utf-8"?>
<!-- chy My session界面 -->
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:layout="http://schemas.android.com/apk/res/com.innovzen.o2chair"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/background"
    android:gravity="center_horizontal" >

    <com.innovzen.ui.MeasureView
        android:id="@+id/mid_left_vertical_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        layout:orientation="true"
        layout:rate_down="42"
        layout:rate_up="1" />

    <com.innovzen.ui.MeasureView
        android:id="@+id/mid_left_vertical_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@id/mid_left_vertical_1"
        layout:orientation="true"
        layout:rate_down="42"
        layout:rate_up="3" />

    <com.innovzen.ui.MeasureView
        android:id="@+id/mid_left_vertical_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@id/mid_left_vertical_2"
        layout:orientation="true"
        layout:rate_down="42"
        layout:rate_up="2" />

    <com.innovzen.ui.MeasureView
        android:id="@+id/mid_left_vertical_4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@id/mid_left_vertical_3"
        layout:orientation="true"
        layout:rate_down="42"
        layout:rate_up="14" />

    <com.innovzen.ui.MeasureView
        android:id="@+id/mid_left_vertical_5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@id/mid_left_vertical_4"
        layout:orientation="true"
        layout:rate_down="42"
        layout:rate_up="2" />

    <com.innovzen.ui.MeasureView
        android:id="@+id/mid_left_vertical_6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@id/mid_left_vertical_5"
        layout:orientation="true"
        layout:rate_down="42"
        layout:rate_up="3" />

    <com.innovzen.ui.MeasureView
        android:id="@+id/mid_top_horizontal_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@id/mid_left_vertical_1"
        layout:orientation="false"
        layout:rate_down="55"
        layout:rate_up="2" />

    <com.innovzen.ui.MeasureView
        android:id="@+id/mid_top_horizontal_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@id/mid_top_horizontal_1"
        layout:orientation="false"
        layout:rate_down="55"
        layout:rate_up="12" />

    <com.innovzen.ui.MeasureView
        android:id="@+id/mid_top_horizontal_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@id/mid_top_horizontal_2"
        layout:orientation="false"
        layout:rate_down="55"
        layout:rate_up="1" />

    <com.innovzen.ui.MeasureView
        android:id="@+id/mid_top_horizontal_4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@id/mid_top_horizontal_3"
        layout:orientation="false"
        layout:rate_down="55"
        layout:rate_up="12" />

    <com.innovzen.ui.MeasureView
        android:id="@+id/mid_top_horizontal_5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@id/mid_top_horizontal_4"
        layout:orientation="false"
        layout:rate_down="55"
        layout:rate_up="1" />

    <com.innovzen.ui.MeasureView
        android:id="@+id/mid_top_horizontal_6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@id/mid_top_horizontal_5"
        layout:orientation="false"
        layout:rate_down="55"
        layout:rate_up="12" />

    <com.innovzen.ui.MeasureView
        android:id="@+id/mid_top_horizontal_7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@id/mid_top_horizontal_6"
        layout:orientation="false"
        layout:rate_down="55"
        layout:rate_up="1" />

    <com.innovzen.ui.MeasureView
        android:id="@+id/mid_top_horizontal_8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@id/mid_top_horizontal_7"
        layout:orientation="false"
        layout:rate_down="55"
        layout:rate_up="12" />

    <com.innovzen.ui.MeasureView
        android:id="@+id/mid_top_horizontal_9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@id/mid_top_horizontal_8"
        layout:orientation="false"
        layout:rate_down="55"
        layout:rate_up="2" />

    <ImageView
        android:id="@+id/beginner"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@id/mid_left_vertical_2"
        android:layout_alignLeft="@id/mid_top_horizontal_2"
        android:layout_alignRight="@id/mid_top_horizontal_2"
        android:layout_alignTop="@id/mid_left_vertical_2"
        android:background="@drawable/selector_icon_beginner" />

    <ImageView
        android:id="@+id/intermadiate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@id/mid_left_vertical_2"
        android:layout_alignLeft="@id/mid_top_horizontal_3"
        android:layout_alignRight="@id/mid_top_horizontal_4"
        android:layout_alignTop="@id/mid_left_vertical_2"
        android:background="@drawable/selector_icon_intermadiate" />

    <ImageView
        android:id="@+id/pro"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@id/mid_left_vertical_2"
        android:layout_alignLeft="@id/mid_top_horizontal_5"
        android:layout_alignRight="@id/mid_top_horizontal_6"
        android:layout_alignTop="@id/mid_left_vertical_2"
        android:background="@drawable/selector_icon_pro" />

    <ImageView
        android:id="@+id/customise"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@id/mid_left_vertical_2"
        android:layout_alignLeft="@id/mid_top_horizontal_7"
        android:layout_alignRight="@id/mid_top_horizontal_8"
        android:layout_alignTop="@id/mid_left_vertical_2"
        android:background="@drawable/selector_icon_customise" />

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@id/mid_left_vertical_3"
        android:layout_alignLeft="@id/mid_top_horizontal_2"
        android:layout_alignRight="@id/mid_top_horizontal_8"
        android:layout_alignTop="@id/mid_left_vertical_3"
        android:gravity="center"
        android:weightSum="4" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center_horizontal"
            android:text="MAX 30 sec"
            android:textColor="#FF6D7172"
            android:textStyle="bold" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center_horizontal"
            android:text="MAX 30 sec"
            android:textColor="#FF6D7172"
            android:textStyle="bold" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center_horizontal"
            android:text="MAX 30 sec"
            android:textColor="#FF6D7172"
            android:textStyle="bold" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center_horizontal"
            android:text="MAX 30 sec"
            android:textColor="#FF6D7172"
            android:textStyle="bold" />
    </LinearLayout>

    <ImageView
        android:id="@+id/image_beginner"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@id/mid_left_vertical_4"
        android:layout_alignLeft="@id/mid_top_horizontal_2"
        android:layout_alignRight="@id/mid_top_horizontal_8"
        android:layout_alignTop="@id/mid_left_vertical_4"
        android:background="@drawable/time_beginner" />

    <ImageView
        android:id="@+id/image_intermediate"
        android:visibility="gone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@id/mid_left_vertical_4"
        android:layout_alignLeft="@id/mid_top_horizontal_2"
        android:layout_alignRight="@id/mid_top_horizontal_8"
        android:layout_alignTop="@id/mid_left_vertical_4"
        android:background="@drawable/time_intermediate" />

    <ImageView
        android:id="@+id/image_pro"
           android:visibility="gone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@id/mid_left_vertical_4"
        android:layout_alignLeft="@id/mid_top_horizontal_2"
        android:layout_alignRight="@id/mid_top_horizontal_8"
        android:layout_alignTop="@id/mid_left_vertical_4"
        android:background="@drawable/time_pro" />

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@id/mid_left_vertical_5"
        android:layout_alignLeft="@id/mid_top_horizontal_2"
        android:layout_alignRight="@id/mid_top_horizontal_8"
        android:layout_alignTop="@id/mid_left_vertical_5"
        android:gravity="center"
        android:weightSum="4" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center_horizontal"
            android:text="Breathe in time"
            android:textColor="#FF6D7172" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center_horizontal"
            android:text="Hold time 3/4"
            android:textColor="#FF6D7172" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center_horizontal"
            android:text="Breathe out time"
            android:textColor="#FF6D7172" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center_horizontal"
            android:text="Hold time"
            android:textColor="#FF6D7172" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@id/mid_left_vertical_6"
        android:layout_alignLeft="@id/mid_top_horizontal_2"
        android:layout_alignRight="@id/mid_top_horizontal_8"
        android:layout_alignTop="@id/mid_left_vertical_6"
        android:gravity="center"
        android:weightSum="4" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_weight="1"
            android:gravity="center_horizontal"
            android:text="@string/full"
            android:textColor="#35A9BB"
            android:visibility="invisible" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_weight="1"
            android:gravity="center_horizontal"
            android:text="@string/full"
            android:textColor="#35A9BB" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_weight="1"
            android:gravity="center_horizontal"
            android:text="@string/empty"
            android:textColor="#35A9BB"
            android:visibility="invisible" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_weight="1"
            android:gravity="center_horizontal"
            android:text="@string/empty"
            android:textColor="#35A9BB" />
    </LinearLayout>

</RelativeLayout>